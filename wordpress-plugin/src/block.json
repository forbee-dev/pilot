{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "microfe/react-component",
  "title": "React Component",
  "category": "widgets",
  "icon": "admin-plugins",
  "description": "Insert a React component from MicroFE",
  "supports": {
    "html": false
  },
  "attributes": {
